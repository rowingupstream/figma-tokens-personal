{
  "global": {
    "colors": {
      "red": {
        "300": {
          "value": "#dd2020",
          "type": "color"
        },
        "500": {
          "value": "#bb5a5a",
          "type": "color"
        }
      },
      "brand": {
        "value": "{colors.red.500}",
        "type": "color"
      }
    },
    "spacing": {
      "small": {
        "value": "16",
        "type": "spacing"
      },
      "xs": {
        "value": "{spacing.small} / {spacing.scale}",
        "type": "spacing"
      },
      "scale": {
        "value": "4",
        "type": "spacing"
      }
    },
    "radius": {
      "default": {
        "value": "16",
        "type": "borderRadius"
      }
    },
    "typograph": {
      "h1": {
        "Bold": {
          "value": {
            "fontFamily": "{fontFamily.default}",
            "fontWeight": "Bold",
            "lineHeight": "AUTO",
            "fontSize": "32",
            "textDecoration": "none",
            "paragraphSpacing": "0",
            "letterSpacing": "0%"
          },
          "type": "typography"
        },
        "Regular": {
          "value": {
            "fontFamily": "{fontFamily.default}",
            "fontWeight": "Regular",
            "lineHeight": "AUTO",
            "fontSize": "18",
            "textDecoration": "none",
            "paragraphSpacing": "0",
            "letterSpacing": "0%"
          },
          "type": "typography"
        }
      }
    },
    "fontFamily": {
      "default": {
        "value": "Roboto Mono",
        "type": "fontFamilies"
      }
    },
    "shadow": {
      "default": {
        "value": [
          {
            "x": "4",
            "y": "4",
            "blur": "8",
            "spread": "0",
            "color": "rgba(0, 0, 0, 0.5)",
            "type": "dropShadow"
          },
          {
            "x": "1",
            "y": "1",
            "blur": "1",
            "spread": "0",
            "color": "rgba(0, 0, 0, 0.25)",
            "type": "dropShadow"
          }
        ],
        "type": "boxShadow"
      }
    }
  },
  "$themes": []
}